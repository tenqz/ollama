{
    "name": "tenqz/ollama",
    "description": "Php library for working with Ollama server.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Tenqz\\Ollama\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Oleg Patsay",
            "email": "smmartbiz@gmail.com"
        }
    ],
    "minimum-stability": "alpha",
    "require": {
        "php": "^7.2|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "phpstan/phpstan": "^1.12",
        "squizlabs/php_codesniffer": "^4.0@RC",
        "friendsofphp/php-cs-fixer": "^3.4"
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "php-cs-fixer fix --dry-run --diff && phpcs",
        "fix-style": "php-cs-fixer fix && phpcbf",
        "analyze": "phpstan analyse"
    }
}
